<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.Scene?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>

<Scene fx:id="splash" xmlns="http://javafx.com/javafx/20.0.1" xmlns:fx="http://javafx.com/fxml/1">
    <AnchorPane prefHeight="250.0" prefWidth="600.0">
      <children>
         <ImageView fitHeight="250.0" fitWidth="600.0" pickOnBounds="true" preserveRatio="true">
            <image>
               <Image url="@splash.jpg" />
            </image>
         </ImageView>
         <Button fx:id="continueToController" layoutX="246.0" layoutY="211.0" mnemonicParsing="false" style="-fx-background-radius: 15;" text="Click To Continue" />
      </children>
    </AnchorPane>
</Scene>
